@{
    var checked_ = "checked";
    foreach (var doc in @Model)
    {
        <div class="col-6 mb-4">
            <div class="card">
                <img onclick="window.open('@doc.Profile_url','_SELF')" style="width:100%" src="@doc.Icon" class="card-img-top" alt="...">
                <div class="card-body" style="padding:0.25rem">
                    <h5 class="card-title text-center" style="font-size:14px; color:#0979C0; margin-top:-5px;"><a href="@doc.Profile_url">@doc.Names[ViewBag.LangId]</a></h5>
                    <div class="text-center"><small>@doc.Dept_name</small></div>
                    <p class="card-text text-center">
                        <span class="fa fa-star @{ if (@doc.Rate > 0) {@checked_ }; } "></span>
                        <span class="fa fa-star @{ if (@doc.Rate > 1) {@checked_ }; }"></span>
                        <span class="fa fa-star @{ if (@doc.Rate > 2) {@checked_ }; }"></span>
                        <span class="fa fa-star @{ if (@doc.Rate > 3) {@checked_ }; }"></span>
                        <span class="fa fa-star @{ if (@doc.Rate > 4) {@checked_ }; } "></span>
                    </p>
                </div>
            </div>
        </div>

    }
}

<script type="text/template" id="tempDoctor">
